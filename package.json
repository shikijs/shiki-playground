    "dev": "vite",
    "prebuild": "npm run copy",
    "build": "vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "^4.4.1",
    "dom-to-image": "^2.6.0",
    "file-saver": "^2.0.5",
    "lz-string": "^1.4.4",
    "shiki": "^0.9.4",
    "shiki-renderer-svg": "^0.9.4",
    "vue": "^3.0.5",
    "vue-router": "4",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@types/dom-to-image": "^2.6.2",
    "@types/file-saver": "^2.0.1",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^15.0.2",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.5",
    "shx": "^0.3.3",
    "typescript": "^4.1.3",
    "vite": "^2.3.3"
  }
}
